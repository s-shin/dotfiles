// 既定値を上書きするには、このファイル内にキー バインドを挿入します
[
    { "key": "cmd+k cmd+n", "command": "workbench.action.focusNextGroup" },
    { "key": "cmd+k cmd+b", "command": "workbench.action.focusPreviousGroup" },
    { "key": "ctrl+d", "command": "deleteRight", "when": "editorTextFocus && !editorReadonly" },
    { "key": "ctrl+u", "command": "extension.vim_ctrl+u", "when": "editorTextFocus && vim.active && !inDebugRepl && vim.mode != 'Insert'" },
    { "key": "ctrl+d", "command": "extension.vim_ctrl+d", "when": "editorTextFocus && vim.active && !inDebugRepl && vim.mode != 'Insert'" }
]
