{
    "editor.fontFamily": "'MyricaM M', 'Ricty Discord', Ricty, Consolas",
    "editor.fontSize": 14,
    "editor.rulers": [
        80
    ],
    "editor.renderIndentGuides": true,
    // useCtrlKeysをtrueにした上でctrl+dをkeybindings.jsonで上書きすると期待の動作になる
    "vim.useCtrlKeys": true,
    "vim.handleKeys": {
        "<C-f>": false,
        "<C-n>": false,
        "<C-e>": false,
        "<C-a>": false,
        "<C-k>": false
    },
